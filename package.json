{
  "name": "trip-overlay",
  "version": "1.0.0",
  "type": "module",
  "description": "Motorbike trip overlay for streaming",
  "main": "index.html",
  "scripts": {
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,css,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,css,json,md}\"",
    "format:html": "js-beautify --html --replace --indent-size 2 \"**/*.html\"",
    "format:all": "pnpm run format && pnpm run format:html"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "js-beautify": "^1.15.4",
    "prettier": "^3.3.3",
    "wrangler": "^4.23.0"
  },
  "keywords": [
    "overlay",
    "streaming",
    "gps",
    "travel"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rtirl/api": "^1.2.1"
  }
